@{
    ViewBag.Title = "Home Page";
}
@model IEnumerable<VehicleInsuranceSem3.BLL.ViewModel.CustomerinfoViewModel>

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>
<button type="button" class="btn btn-primary"><a href="@Url.Action("ViewAll")">view</a></button>
<div class="container-fluid">
    <table>
        <thead>
            <tr>
                <th>Customer Name</th>
                <th>Username</th>
                <th>Dob</th>
                <th>Email</th>
            </tr>

        </thead>
        <tbody>
            @if (Session["customerList"] != null)
            {
                var ls = (List<VehicleInsuranceSem3.BLL.ViewModel.CustomerinfoViewModel>)Session["customerList"];
                foreach (var item in ls)
                {
                    <tr>
                        <td>@item.name</td>
                        <td>@item.username</td>
                        <td>@item.dob</td>
                        <td>@item.email</td>
                    </tr>
                }
            }


        </tbody>
    </table>

</div>