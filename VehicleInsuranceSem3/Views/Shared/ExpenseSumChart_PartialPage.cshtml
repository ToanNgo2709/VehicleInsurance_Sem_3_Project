
<script>
    var expenseChart = new CanvasJS.Chart("companyExpenseChart", {
            animationEnabled : true,
            theme: "dark1",
            title: {
                text: "Total Cost of Company Expense"
            },
            axisY: {
                title: "No. of Money"
            },
            data: [{
                type: "column",
                dataPoints: @Html.Raw(ViewBag.DataPoints)
            }]
    });
    expenseChart.render();


</script>